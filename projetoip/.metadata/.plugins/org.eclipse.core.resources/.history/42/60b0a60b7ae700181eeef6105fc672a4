package pessoas;

import java.util.*;

public class Main {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		RepositorioContasLista banco = new RepositorioContasLista();
		while (in.hasNext()) {
			String cpf = in.nextLine();
			String nome = in.nextLine();
			String idadeString = in.nextLine();
			int idade = Integer.parseInt(idadeString);
			ContaFuncionario bem = new ContaFuncionario(cpf, nome, idade);
			banco.inserir(bem, banco.existe(cpf));
			bem.creditar(100);
			bem.creditar(100);
			bem.creditar(100);
			bem.creditar(100);
			bem.creditar(100);
			bem.creditar(100);
			bem.creditar(100);
			bem.creditar(100);
			bem.creditar(100);
			bem.creditar(100);
			bem.creditar(100);
			String resultado = "";
			System.out.print(banco.imprimir(banco.procurar(cpf)));
		}
		;
		

	}

}
